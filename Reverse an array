import  java.util.*;
public class Reverse_array {

	public static void main(String[] args) {
		Scanner sc=new   Scanner(System.in);
		int n=sc.nextInt();
		int a[]=new int[n];
		for(int i=0;i<n;i++)
		{
			a[i]=sc.nextInt();
			
		}
		int j=n-1;
		for(int i=0;i<n/2;)
			
		{
			int temp=a[i];
			
			a[i]=a[j];
			a[j]=temp;
			i++;
			j--;
		}
	
		
	for(int i=0;i<n;i++)
	{
		System.out.println(a[i]);
	}

}
}
